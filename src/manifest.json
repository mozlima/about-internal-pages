{
	"name": "About://Internal Pages",
	"version": "2.0",
	"manifest_version": 2,
	"homepage_url": "http://moisesplima.blogspot.com.br/about-internal-pages",
	"default_locale": "en",
	"description": "__MSG_extension_short_description__",
	"developer": {
		"name": "Moises Lima",
		"url": "http://moisesplima.blogspot.com.br"
	},
	"icons": {
		"16": "images/ip-icon16.png",
		"128": "images/ip-icon128.png"
	},
	"background": {
		"scripts": ["bg.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": {
			"19": "images/ip-icon19.png",
			"38": "images/ip-icon38.png"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"chrome://favicon/"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}